{
  "extends": ["airbnb", "next/core-web-vitals"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/function-component-definition": "off",
    "import/extensions": [
      "error",
      "ignorePackages",
      {
      "js": "never",
      "jsx": "never",
      "ts": "never",
      "tsx": "never"
      }
    ]
  }
}
